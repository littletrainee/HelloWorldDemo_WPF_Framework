<Window x:Class="HelloWorldDemo_WPF_Framework.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:HelloWorldFolder="clr-namespace:HelloWorldDemo_WPF_Framework.HelloWorldFolder"
        xmlns:CountToTenFolder="clr-namespace:HelloWorldDemo_WPF_Framework.CountToTenFolder"
        xmlns:CurrentTimeFolder="clr-namespace:HelloWorldDemo_WPF_Framework.CurrentTimeFolder"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
    <HelloWorldFolder:HelloWorldController/>
    <CountToTenFolder:CountToTenControl/>
    <CurrentTimeFolder:CurrentTimeControl/>
  </Grid>
</Window>
